{
  "manifest_version": 2,

  "name": "Diary",
  "description": "Post event info to a shared calendar.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://oauth.io"
  ]
}
